-- update
function _update()

 if page==0 and trans_title==false and not titlesfx_played and btnp(4) then
 	trans_title=true
 	sfx(7)
 	titlesfx_played=true
 end
 
 if trans_title do
 	grass+=1
 	mini_face_me+=1
 	startbutton-=3
 	title_sun_white-=2
 	title_sun_yello-=2
 	
 	
 
 	
 	
 	
 	if grass>180 then 
	 	trans_title=false
	 	page=1
 	end
 	
 	
 	
 	
 	
 end
 
 
 if page==1 then
  
 		cloudx1+=0.2
 		cloudx2+=0.4
 		
 		if cloudx1>125 then
 			cloudx1=-90
 		end
 		
 		if cloudx2>125 then
 			cloudx2=-50
 		end
 		
 		
 		
 		
 		
 		
 		--‚ùé input for antonia
 		if btnp(4) and ar_sel==false and an_sel==false then
 			sfx(4)
 			an_sel=true
 		end
 			 
 			
 			
			if an_sel==true and an_reached==false then
				antonia=2
			 an_spr-=2.5 
			 
			 if an_spr<43 then
			 	an_reached=true
			 
			 end
			end
			 

			 
			
			
			if an_reached then
				
					an_spr+=accel
					accel+=0.5
					
					
					an_x-=1
					an_y-=1
					
					an_fall=16
				
				
				if an_spr > 140 then
					page=2
				end
				
			end
	 	
 				
 				
 	
 		
 		
 		
 		
 		--üÖæÔ∏è input for arron
 		if btnp(5) and an_sel==false and ar_sel==false then
 			sfx(4)
 			ar_sel=true
 		end
 			 
 			
 			
			if ar_sel==true and ar_reached==false then
				arron=17
			 ar_spr-=2.5 
			 
			 if ar_spr<43 then
			 	ar_reached=true
			 
			 end
			end
			 

			 
			
			
			if ar_reached then
				
					ar_spr+=accel
					accel+=0.5
					
					
					ar_x-=1
					ar_y-=1
					
					ar_fall=16
				
				
				if ar_spr > 128 then
					page=2
				end
				
			end
 	
 end
 
 
 
 
 
 
 -- page 2
 
 
 
 --level 1 arron
 if page==2 and ar_sel then
 	
		--left
		if btn(0) then
			arron=1
		end
		--right
		if btn(1) then
			arron=17
		end
		--up
		if btn(2) then
			if arron==17 or arron==19 then
			arron=49
			
			end
			
			if arron==1 or arron==20 then
				arron=33
			end
			
		end
		--down/crouch
		if btn(3) then
			if arron==17 or arron==49 then
				arron=19
			end
			
			if arron==1 or arron==33 then
				arron=20
			end
			
		end
	end
 
 
 
 
 
 
 
 --level 1 antonia
 if page==2 and an_sel then
 	
		--left
		if btn(0) then
			antonia=18
		end
		--right
		if btn(1) then
			antonia=2
		end
		--up
		if btn(2) then
			--right
			if antonia==2 or antonia==35 then
			antonia=34
			
			end
			--left
			if antonia==18 or antonia==36 then
				antonia=50
			end
			
		end
		--down/crouch
		--right
		if btn(3) then
			if antonia==2 or antonia==34 then
				antonia=35
			end
			--left
			if antonia==18 or antonia==50 then
				antonia=36
			end
			
		end
	end
 
 
 

end
